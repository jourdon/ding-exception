{
    "name": "jourdon/ding-exception",
    "description": "Notify Laravel exceptions via DingTalk",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.1",
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "jourdon",
            "email": "admin@qiehe.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jourdon\\DingException\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jourdon\\DingException\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jourdon\\DingException\\DingExceptionServiceProvider"
            ],
            "aliases": {
                "Slug": "Jourdon\\DingException\\DingException"
            }
        }
    }
}
